<!-- answer.xml.fwx | keyid:<%= q_key.keyid %> -->
    <answer fraction="<%= int(100*q_key.k_worth / qques.qs_worth) %>" format="moodle_auto_format">
      <text><%= q_key.K_text %></text>
      <feedback format="html">
        <text><![CDATA[<%= iif(not (empty(q_key.K_feedback) or isnull(q_key.K_feedback) ), q_key.K_feedback,  iif(q_key.k_worth >= qques.qs_worth, qques.yup,  qques.nope  ) ) %>]]></text>
      </feedback>
    </answer>
